    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/typography": "^0.5.14",
    "autoprefixer": "^10.4.20",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "wrangler": "^3.72.2"
  },
  "type": "module",
  "dependencies": {
    "bits-ui": "^0.21.13",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.436.0",
